

@{
    ViewData["Title"] = "Detalle productos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="mt-4 border rounded bg-white">
    <table class="table mb-0 table-bordered table-striped">
        <thead>
            <tr>
                <th>Insumo</th>
                <th>Cantidad</th>
                <th>Medida</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var detalle in ViewBag.Detalles)
            {
                <tr>
                    <td>@detalle.IdInsumoNavigation.Nombre</td>
                    <td>@detalle.Cantidad</td>
                    <td>@detalle.Medida</td>
                </tr>
            }
        </tbody>
    </table>

</div>
   <div class="d-flex justify-content-center pt-3">
    <a class="btn btn-dark " asp-action="Index">Volver</a>
</div>

